{
    "name": "Very Good Co Sneak Peak",
    "description": "Create a layover ontop of products that shows Title and Price.",
    "version": "0.1.1",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["https://verygoods.co/*"],
        "js": ["jq.2.min.js", "main.js"]
    }], "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ], "browser_action": {
        "default_icon": "icon128.png"
    }, "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png"
    }
}